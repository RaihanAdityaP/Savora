{
  "buildCommand": "bash build.sh",
  "outputDirectory": "build/web",
  "installCommand": "echo 'No install needed'",
  "routes": [
    {
      "src": "/recipe/([a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12})",
      "dest": "/recipe/share.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}